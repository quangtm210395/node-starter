# local dev stg prod
NODE_ENV=

#
# Server type for producer or worker or both
# value: producer, worker (spread with commas - can use one or all of them)
# producer - allow produce request like http, emmit event, socket,....
# worker - only listener event and resolve them
# cron - run the cron jobs
# bot - telegram bot
#
SERVER_TYPE=producer
JOB_DEFINITIONS=
SERVICE_NAME=sf-starter
#
# mongodb
#
MONGODB_URI=mongodb://root:password123@localhost:27017/demo?authSource=admin&ssl=false


#
# App
#
APP_NAME=SF Starter
APP_SCHEMA=http
APP_HOST=localhost
APP_PORT=3001
APP_ROUTE_PREFIX=/api
APP_EXTERNAL_ROUTE_PREFIX=/api
APP_EXTERNAL_PORT=3001
APP_BE_ROUTE_PREFIX=/api

#
# Swagger
#
SWAGGER_ENABLED=true
SWAGGER_ROUTE=/swagger
SWAGGER_API_DOCS=/api-docs

#
# Status Monitor
#
MONITOR_ENABLED=true
MONITOR_ROUTE=/monitor
MONITOR_USERNAME=admin
MONITOR_PASSWORD=1234

#
# LOGGING
#
LOG_LEVEL=debug
# LOG_JSON=true

GRAPHQL_PATH=/graphql
GRAPHQL_EDITOR=true
GRAPHQL_SUBSCRIPTION_PATH=/subscriptions
#
# PostgreSQL DATABASE
#
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=2002
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=postgres
DB_SCHEMA=demo
DB_SYNCHRONIZE=true
DB_LOGGING=all
DB_LOGGER=advanced-console

